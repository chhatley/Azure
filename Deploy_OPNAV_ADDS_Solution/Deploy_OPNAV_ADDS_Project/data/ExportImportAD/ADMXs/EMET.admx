      <supportedOn ref="SUPPORTED_ProductOnly" />
      <enabledList>
	   	<!-- Wordpad -->
        <item key="Software\Policies\Microsoft\EMET\Defaults" valueName="*\Windows NT\Accessories\wordpad.exe">
          <value>
            <string></string>
          </value>
        </item>

        <!-- Office Suites 2003,2007,2010,2013 and Office365 -->
